\DeclareOptionBeamer{compress}{\beamer@compresstrue}
\ProcessOptionsBeamer

\mode<presentation>


\useinnertheme[shadow=true]{rounded}
\useoutertheme[footline=institutetitle,subsection=false]{miniframes}


%\useoutertheme[footline=authorinstitutetitle,subsection=false]{miniframes}
\usecolortheme{dolphin}


\setbeamercolor*{block title}{fg=white,bg=blue!80}
\setbeamercolor*{block title alerted}{fg=white,bg=red!50!black}
\setbeamercolor*{block title example}{fg=white,bg=green!60!black}

\setbeamercolor*{block body}{fg=black,bg=black!5}
\setbeamercolor*{block body alerted}{fg=black,bg=red!03}
\setbeamercolor*{block body example}{fg=black,bg=black!10}

\setbeamercolor*{separation line}{}



\setbeamercolor{titlelike}{parent=structure}

\setbeamerfont{block title}{size={}}

\mode
<all>
