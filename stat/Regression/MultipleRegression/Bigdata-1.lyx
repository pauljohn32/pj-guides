\begin_layout Section
Multiple Regression
\end_layout

\begin_layout BeginFrame
OLS with Many Predictors
\end_layout

\begin_layout Itemize
Theory? SAME as elementary.
 Just add more predictors.
 
\end_layout

\begin_layout Standard
\align center
\begin_inset Formula $y_{i}=b_{0}+b_{1}x1_{i}+b_{2}x2_{i}+b_{3}x3_{i}+\ldots+e_{i}$
\end_inset


\end_layout

\begin_layout Itemize
Goal? SAME.
 Find a vector of parameter estimates 
\begin_inset Formula $\widehat{b}=[\widehat{b_{0}},\widehat{b_{1}},\widehat{b_{2}},\widehat{b_{3}},\ldots,].$
\end_inset

 Use those to calculate a 
\begin_inset Quotes eld
\end_inset

predicted value
\begin_inset Quotes erd
\end_inset

 for each i:
\begin_inset Formula 
\begin{equation}
\widehat{y_{i}}=\widehat{b_{0}}+\widehat{b_{1}}x1+\widehat{b_{2}}x2_{i}+\label{eqa}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Strategy? SAME.
 Minimize the Sum of Squares, 
\begin_inset Quotes eld
\end_inset

total amount of unexplained variation
\begin_inset Quotes erd
\end_inset

 in the variable 
\begin_inset Formula $y_{i}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
\min_{\widehat{b}}S(\widehat{b_{0}},\widehat{b_{1}},\widehat{b_{2}})=\sum_{i=1}^{N}(y_{i}-\widehat{y_{i}})^{2}\label{eqb}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\sum_{i=1}^{N}(y_{i}-\widehat{b_{0}}-\widehat{b_{1}}x1_{i}-\widehat{b_{2}}x2_{i})^{2}
\]

\end_inset


\end_layout

\begin_layout BeginFrame
When The Dust Clears, Solutions
\end_layout

\begin_layout Itemize
I would prefer to write out the math in matrix algebra.
 
\end_layout

\begin_layout Itemize
Here's why.
 Look at this horrible formula with ordinary 
\begin_inset Quotes eld
\end_inset

scalar
\begin_inset Quotes erd
\end_inset

 algebra.
 
\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Tabular
<lyxtabular version="3" rows="3" columns="1">
<features tabularvalignment="middle">
<column alignment="center" valignment="top" width="0pt">
<row>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout

\end_layout

\end_inset
</cell>
</row>
<row bottomspace="1cm">
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\hat{b}_{1}=\frac{\left(\sum(x1_{i}-\overline{x1})(y_{i}-\bar{y})\right)\left(\sum(x2_{i}-\overline{x2})^{2}\right)-\left(\sum(x2_{i}-\overline{x2})(y_{i}-\bar{y})\right)\left(\sum(x1_{i}-\overline{x1})(x2_{i}-\overline{x2}\right)}{\left(\sum(x1_{i}-\overline{x1})^{2}\right)\left(\sum(x2{}_{i}-\overline{x2})^{2}\right)-\left(\sum(x1_{i}-\overline{x1})(x2_{i}-\overline{x2})\right)^{2}}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
<row bottomspace="1cm">
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\widehat{Var(\hat{b}_{1})}=\frac{\widehat{\sigma_{e}^{2}}}{\sum(x1_{i}-\overline{x1})^{2}(1-r_{x1,x2})}=\frac{MSE}{(N-1)Var[x1](1-r_{x1,x2})}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
</lyxtabular>

\end_inset


\end_layout

\end_deeper
\begin_layout BeginFrame
The formula for 
\begin_inset Formula $\hat{b}_{2}$
\end_inset

 looks similar
\end_layout

\begin_layout Standard
\begin_inset Tabular
<lyxtabular version="3" rows="3" columns="1">
<features tabularvalignment="middle">
<column alignment="center" valignment="top" width="0pt">
<row bottomspace="1cm">
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\hat{b}_{2}=\frac{\left(\sum(x2_{i}-\overline{x2})(y_{i}-\bar{y})\right)\left(\sum(x1{}_{i}-\overline{x1})^{2}\right)-\left(\sum(x1{}_{i}-\overline{x1})(y_{i}-\bar{y})\right)\left(\sum(x1_{i}-\overline{x1})(x2_{i}-\overline{x2}\right)}{\left(\sum(x1_{i}-\overline{x1})^{2}\right)\left(\sum(x2{}_{i}-\overline{x2})^{2}\right)-\left(\sum(x1_{i}-\overline{x1})(x2_{i}-\overline{x2})\right)^{2}}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
<row bottomspace="1cm">
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\widehat{Var(\hat{b}_{2})}=\frac{\widehat{\sigma_{e}^{2}}}{\sum(x2_{i}-\overline{x2})^{2}(1-r_{x1,x2})}=\frac{MSE}{(N-1)Var[x2](1-r_{x1,x2})}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\hat{b}_{0}=\bar{y}-\hat{b}_{1}\overline{x1}-\hat{b}_{2}\overline{x2}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
</lyxtabular>

\end_inset


\end_layout

\begin_layout BeginFrame
Gauss Markov Theorem: OLS is Best Linear Unbiased Estimator (BLUE)
\end_layout

\begin_layout Itemize
IF
\end_layout

\begin_deeper
\begin_layout Itemize
you fit the 
\begin_inset Quotes eld
\end_inset

right
\begin_inset Quotes erd
\end_inset

 model, 
\begin_inset Formula $y_{i}=b_{0}+b_{1}x1_{i}+b_{2}x2_{i}+\ldots+e_{i}$
\end_inset

 
\end_layout

\begin_layout Itemize
and the error term assumptions are satisfied, namely
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $E[e_{i}]=0$
\end_inset

, 
\begin_inset Formula $Var[e_{i}]=E[e_{i}^{2}]=\sigma_{e}^{2}$
\end_inset

, 
\begin_inset Formula $Cov(e_{i},e_{j})=0$
\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Itemize
THEN The OLS Estimates 
\begin_inset Formula $\hat{b}_{j}$
\end_inset

 (
\begin_inset Formula $j=0,1,2,...,p$
\end_inset

) are 
\end_layout

\begin_deeper
\begin_layout Itemize
unbiased, 
\begin_inset Formula $E[\hat{b}_{j}]=b_{j}$
\end_inset


\end_layout

\begin_layout Itemize
consistent: (generallly), as N grows, the gap between 
\begin_inset Formula $\hat{b}_{j}$
\end_inset

 and 
\begin_inset Formula $b_{j}$
\end_inset

 shrinks
\end_layout

\begin_layout Itemize
efficient: lower variance than any other linear estimator
\end_layout

\end_deeper
\begin_layout Itemize
\begin_inset Formula $\hat{b}_{j}/std.err(\hat{b}_{j})$
\end_inset

 is distributed as a 
\begin_inset Formula $t$
\end_inset

 statistic
\end_layout

