\begin_layout Section
Graphical Reminder
\end_layout

\begin_layout BeginFrame
The Theoretical model
\end_layout

\begin_layout Itemize
An output variable is created as the weighted sum of several 
\begin_inset Quotes eld
\end_inset

predictors
\begin_inset Quotes erd
\end_inset

 and a random error term
\end_layout

\begin_layout Itemize
Formally
\begin_inset Formula 
\begin{equation}
y_{i}=b_{0}+b_{1}x1_{i}+b_{2}x2_{i}+\ldots+e_{i}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
where
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $b_{0},$
\end_inset

 
\begin_inset Formula $b_{1}$
\end_inset

, 
\begin_inset Formula $b_{2}$
\end_inset

 are real-valued constants
\end_layout

\begin_layout Itemize
\begin_inset Formula $e_{i}$
\end_inset

 is drawn from a distribution for which 
\begin_inset Formula $E[e_{i}]=0$
\end_inset

 and has constant variance 
\begin_inset Formula $Var[e_{i}]=\sigma_{e}^{2}$
\end_inset


\end_layout

\begin_layout Itemize
None of the error terms are correlated with each other, 
\begin_inset Formula $Cov(e_{i},e_{j})=0$
\end_inset

 and they are uncorrelated with the predictors, 
\begin_inset Formula $Cov(x_{i},e_{i})=0$
\end_inset


\end_layout

\end_deeper
