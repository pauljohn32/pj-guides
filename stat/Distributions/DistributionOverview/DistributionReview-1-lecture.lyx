\begin_layout Standard
Suppose 
\begin_inset Formula $x_{i}\sim N(\mu,\sigma^{2})$
\end_inset

.
 Then
\end_layout

\begin_layout Itemize
\begin_inset Formula $E[x_{i}]=\mu$
\end_inset

 (The expected value of 
\begin_inset Formula $x_{i}$
\end_inset

 is the parameter 
\begin_inset Formula $\mu$
\end_inset

)
\end_layout

\begin_layout Itemize
\begin_inset Formula $Var[x_{i}]=\sigma^{2}$
\end_inset

 (The variance of 
\begin_inset Formula $x_{i}$
\end_inset

 is the parameter 
\begin_inset Formula $\sigma^{2}$
\end_inset

)
\end_layout

\begin_layout BeginFrame
\begin_inset Formula $N(0,1)$
\end_inset

 is called the 
\begin_inset Quotes eld
\end_inset

Standard
\begin_inset Quotes erd
\end_inset

 Normal
\end_layout

\begin_layout Itemize
Recall 
\begin_inset Formula $Z$
\end_inset

 statistic.
\begin_inset Formula 
\begin{equation}
Z_{i}=\frac{x_{i}-\mu}{\sigma}\label{eq:Zstatistic}
\end{equation}

\end_inset


\begin_inset Newline newline
\end_inset

 
\begin_inset Formula $Z_{i}\sim N(0,1$
\end_inset

).
\end_layout

\begin_layout Itemize
Recover 
\begin_inset Formula $x_{i}$
\end_inset

 from 
\begin_inset Formula $Z_{i}$
\end_inset


\begin_inset Formula 
\begin{equation}
x_{i}=\mu+Z_{i}\sigma.\label{eq:XfromZ}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Standard Normal Tables in all stat books (used to be...)
\end_layout

